<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qfedu.examsys.dao.ScoreManageDao">
    <insert id="add" parameterType="scoreManage">
        insert into score_manage (studentId, applyManageId, score) values (#{studentId},#{applyManageId},#{score})
    </insert>
</mapper>