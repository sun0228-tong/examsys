               selectByExampleQueryId="false">
        </table>
        <table schema="examsys" tableName="short_manage" domainObjectName="ShortManage"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false">
        </table>
        <table schema="examsys" tableName="short_question" domainObjectName="ShortQuestion"
